          url: request.URL,","            options: request.Options,","            formData: isContentful(request.FormData) ? request.FormData : undefined","        })}`, { keepalive: true, cache })","            .then(async (response) => {","                const { ok, status, headers, body } = await response.json();","                result.fromJSObject({","                    ok,","                    status,","                    headers,","                    body,","                });","            })","            .catch((e) => {","                logger.error(`Couldn't execute request '${requestName}': ${e}`);","            })","    );","} else {","    if (hasFormData && request.Options.method !== 'GET' && request.Options.method !== 'HEAD') {","        const fd = request.Options.body = new FormData();","        for (const key of Object.keys(request.FormData)) {","            fd.append(key, request.FormData[key])","        }","    }","","    eventsFunctionContext.task = new gdjs.PromiseTask(","        fetch(request.URL, { keepalive: true, ...request.Options })","            .then((response) => {","                const id = advancedHTTP.id++;","                const { ok, status, headers } = response;","                result.fromJSObject({","                    id,","                    ok,","                    status,","                    headers: Object.fromEntries(headers.entries()),","                });","                result.getChild(\"id\").setNumber(id);","                advancedHTTP.responses.set(id, response);","                setTimeout(() => advancedHTTP.responses.delete(id), 10_000);","            })","            .catch((e) => {","                logger.error(`Couldn't execute request '${requestName}': ${e}`);","            })","    );","}"],"parameterObjects":"","useStrict":true,"eventsSheetExpanded":false}],"parameters":[{"description":"Request to execute","name":"Request","supplementaryInformation":"sceneRequest","type":"identifier"},{